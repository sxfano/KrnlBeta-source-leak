<UserControl
	x:Class="KrnlUI.Controls.FolderDisplay" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:KrnlUI.Controls"
	Height="48">
	<StackPanel
		VerticalAlignment="Center"
		Height="48"
		Orientation="Horizontal"
		Margin="0">
		<Label
			Name="EntryName"
			Content="Folder"
			VerticalContentAlignment="Center"
			Padding="0"
			Foreground="#FFD5D5D5"
			FontFamily="/KrnlUI;component/Fonts/#Inter" />
		<Canvas
			Name="svg240"
			Width="5"
			Height="7"
			Margin="10,1,10,0">
			<UIElement.RenderTransform>
				<TranslateTransform
					X="0"
					Y="0" />
			</UIElement.RenderTransform>
			<Path xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
				Name="path209"
				Stroke="#FF7C7C7C">
				<Path.Data>
					<PathGeometry
						Figures="M0.5 6.5L3.5 3.5L0.5 0.5"
						FillRule="NonZero" />
				</Path.Data>
			</Path>
		</Canvas>
	</StackPanel>
</UserControl>
