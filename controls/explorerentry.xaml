	Width="208"
	Height="165"
	HorizontalAlignment="Center"
	VerticalAlignment="Center"
	Focusable="True"
	MouseDown="UserControl_MouseLeftButtonUp">
	<FrameworkElement.Triggers>
		<EventTrigger
			RoutedEvent="UIElement.MouseLeftButtonDown">
			<BeginStoryboard>
				<Storyboard
					Name="CardBorderDownSB"
					TargetProperty="BorderBrush.Color">
					<ColorAnimation
						Name="CardBorderDown"
						Duration="0:0:0.1"
						Storyboard.TargetName="CardBorder"
						To="#FF18A0FB" />
				</Storyboard>
			</BeginStoryboard>
		</EventTrigger>
		<EventTrigger
			RoutedEvent="UIElement.MouseLeftButtonUp">
			<BeginStoryboard>
				<Storyboard
					Name="CardBorderUpSB"
					TargetProperty="BorderBrush.Color">
					<ColorAnimation
						Name="CardBorderUp"
						Duration="0:0:0.1"
						Storyboard.TargetName="CardBorder"
						To="#11FFFFFF" />
				</Storyboard>
			</BeginStoryboard>
		</EventTrigger>
	</FrameworkElement.Triggers>
