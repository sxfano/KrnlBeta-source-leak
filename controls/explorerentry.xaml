						Text="TextBox"
						VerticalAlignment="Top"
						Width="148"
						Background="#FF1F1F1F"
						Foreground="#FFD5D5D5"
						FontFamily="/KrnlUI;component/Fonts/#Inter"
						FontSize="11"
						BorderThickness="0"
						CaretBrush="#FFA8A8A8"
						Visibility="Hidden"
						BorderBrush="{x:Null}"
						LostFocus="EntryEdit_LostFocus" />
				</Grid>
			</Grid>
		</Border>
	</Grid>
</UserControl>
